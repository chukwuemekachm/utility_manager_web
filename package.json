{
  "name": "utility_manager_web",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/chukwuemekachm/utility_manger_web.git",
  "author": "chukwuemekachm <chukwuemekachm@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:dev": "rm -rf dist && webpack-dev-server --env.mode development",
    "build:prod": "rm -rf dist && webpack --env.mode production",
    "prestart": "yarn build:prod",
    "start": "node server.js",
    "dev": "yarn build:dev && rm -rf dist && webpack-dev-server --env.mode development --open --hot",
    "storybook": "start-storybook",
    "ts-test": "tsc --noEmit --strict",
    "lint-test": "rm -rf dist && tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
    "test": "exit 0",
    "lint": "rm -rf dist && tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-notes": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux": "^3.6.0",
    "@types/redux-saga": "^0.10.5",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.1",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "@sentry/browser": "^5.6.1",
    "@storybook/react": "^5.2.8",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "connected-react-router": "^6.5.2",
    "history": "^4.10.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "validatorjs": "^3.17.1",
    "webpack-dev-server": "^3.10.1"
  }
}
